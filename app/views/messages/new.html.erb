<%= form_for @message, url: create_message_url do |f| %>
  <%= notice %>
  <%= @message.errors.full_messages.join(', ') %>

  <%= f.text_field :nome, placeholder: 'nome' %>
  <%= f.email_field :email, placeholder: 'email' %>
  <%= f.text_area :associado, placeholder: 'associado' %>
  <%= f.text_area :advogado, placeholder: 'advogado' %>
  <%= f.submit 'Send' %>
<% end %>
